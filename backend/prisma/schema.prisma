datasource db {
    provider = "postgresql"
    url = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

model User {
    id String @id @default(uuid())
    username String
    login String @unique
    password String
    img String
    nickname String @unique @db.VarChar(255)
}
